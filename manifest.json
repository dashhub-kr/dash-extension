{
  "manifest_version": 3,
  "name": "대시허브 (DashHub)",
  "version": "1.7.5",
  "description": "BOJ 및 SWEA에서 제출한 코드를 로컬 스토리지에 기록하고 사용자가 연결한 개인 GitHub 리포지토리에 자동으로 커밋해 줍니다.",
  "author": "utact",
  "homepage_url": "https://github.com/utact/dash",
  "icons": {
    "16": "assets/icon.png",
    "48": "assets/icon.png",
    "128": "assets/icon.png"
  },
  "action": {
    "default_icon": "assets/icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "scripts/background.js"
  },
  "permissions": [
    "unlimitedStorage",
    "storage",
    "declarativeNetRequest",
    "declarativeNetRequestWithHostAccess"
  ],
  "host_permissions": [
    "https://www.acmicpc.net/",
    "https://swexpertacademy.com/",
    "https://github.com/",
    "https://solved.ac/api/v3/*"
  ],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  },
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "libs/jquery-3.3.1.min.js",
        "libs/semantic.min.js",
        "popup.html",
        "popup.js",
        "welcome.html",
        "welcome.js"
      ]
    }
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "scripts/bridge.js"
      ],
      "run_at": "document_start"
    },
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "scripts/common/util.js",
        "scripts/common/github.js",
        "scripts/common/authorize.js"
      ],
      "run_at": "document_idle"
    },
    {
      "matches": [
        "https://www.acmicpc.net/*"
      ],
      "css": [
        "css/content.css"
      ],
      "js": [
        "libs/sha1.min.js",
        "libs/jszip.min.js",
        "libs/FileSaver.min.js",
        "scripts/common/util.js",
        "scripts/common/toast.js",
        "scripts/common/formatting.js",
        "scripts/common/github.js",
        "scripts/common/authorize.js",
        "scripts/common/storage.js",
        "scripts/common/enable.js",
        "scripts/common/upload.js",
        "scripts/boj/variables.js",
        "scripts/boj/storage.js",
        "scripts/boj/util.js",
        "scripts/boj/parsing.js",
        "scripts/boj/boj.js"
      ],
      "run_at": "document_idle"
    },
    {
      "matches": [
        "https://swexpertacademy.com/*"
      ],
      "css": [
        "css/content.css"
      ],
      "js": [
        "libs/sha1.min.js",
        "scripts/common/util.js",
        "scripts/common/formatting.js",
        "scripts/common/github.js",
        "scripts/common/authorize.js",
        "scripts/common/storage.js",
        "scripts/common/enable.js",
        "scripts/common/upload.js",
        "scripts/swea/storage.js",
        "scripts/swea/variables.js",
        "scripts/swea/util.js",
        "scripts/swea/parsing.js",
        "scripts/swea/swexpertacademy.js"
      ],
      "run_at": "document_idle"
    }
  ]
}